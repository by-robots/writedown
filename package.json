{
  "name": "writedown",
  "version": "1.0.0",
  "description": "WriteDown - A really simple blogging platform.",
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "fix": "standard --fix; eslint --fix --ext .vue src",
    "lint": "standard; eslint --ext .vue src",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.dev.js",
    "precommit-msg": "echo 'Running pre-commit checks.'",
    "prod": "webpack --config webpack.prod.js"
  },
  "pre-commit": [
    "precommit-msg",
    "fix"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:byrobots/writedown.git"
  },
  "author": "By Robots",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 version",
    "Firefox ESR"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@babel/runtime": "^7.2.0",
    "axios": "^0.18.0",
    "core-js": "^3.0.0",
    "navigo": "^7.1.0",
    "normalize.css": "^8.0.0",
    "qs": "^6.7.0",
    "simplemde": "^1.11.0",
    "vue": "^2.6.0",
    "vue-flatpickr-component": "^8.1.0",
    "vuex": "^3.1.0",
    "webpack-manifest-plugin": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "autoprefixer": "^9.4.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.0",
    "sass-loader": "^7.1.0",
    "standard": "^12.0.0",
    "style-loader": "^0.23.0",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.0"
  }
