{
    "name": "byrobots/writedown",
    "description": "Purposeful blogging.",
    "type": "project",
    "license": "MIT",
    "require": {
        "container-interop/container-interop": "^1.2",
        "doctrine/orm": "^2.6",
        "league/commonmark": "^0.17",
        "league/container": "^2.4",
        "league/html-to-markdown": "^4.6",
        "league/route": "^3.0",
        "robmorgan/phinx": "^0.9.2",
        "vlucas/phpdotenv": "^2.4",
        "vlucas/valitron": "^1.4",
        "zendframework/zend-diactoros": "^1.7"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "WriteDown\\": "writedown/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
