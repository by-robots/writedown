{
    "name": "byrobots/writedown",
    "description": "Purposeful blogging.",
    "type": "project",
    "license": "MIT",
    "require": {
        "byrobots/writedown-core": "dev-master",
        "slim/php-view": "^2.2",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {},
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1"
        },
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/byrobots/writedown-core"
        },
        {
            "type": "vcs",
            "url": "https://github.com/byrobots/validation"
        }
    ]
}
